pipeline {
  agent any

  stages {
    stage("Initial Cleanup") {
      steps {
        deleteDir()
      }
    }

    stage("Build") {
      steps {
        echo "Building Stage"
      }
    }

    stage("Test") {
      steps {
        echo "Testing Stage"
      }
    }

    stage("Package") {
      steps {
        echo "Packaging App"
      }
    }

    stage("Deploy") {
      steps {
        echo "Deploy to Dev"
      }
    }

    stage("Clean up") {
      steps {
        cleanWS()
      }
    }
  }
}
